<Window x:Class="FinalTerm_Project_EMS.AttendanceLogin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FinalTerm_Project_EMS"
        mc:Ignorable="d"
        Title="AttendanceLogin" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtEmail" HorizontalAlignment="Left" Margin="135,56,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label Name="lblemail" Content="Email" HorizontalAlignment="Left" Margin="57,54,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnLogin" Content="Login" HorizontalAlignment="Left" Margin="180,144,0,0" VerticalAlignment="Top" Click="btnLogin_Click"  />
        <Label Name="lblpass" Content="Password" HorizontalAlignment="Left" Margin="57,84,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtPass" HorizontalAlignment="Left" Margin="135,86,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
