<Window x:Class="FinalTerm_Project_EMS.EMS_Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FinalTerm_Project_EMS"
        mc:Ignorable="d"
        Title="EMS_Login" Height="450" Width="800">
    <Grid>
        <Label Content="Email" HorizontalAlignment="Left" Height="33" Margin="225,179,0,0" VerticalAlignment="Top" Width="72"/>
        <Label Content="Password" HorizontalAlignment="Left" Height="33" Margin="225,217,0,0" VerticalAlignment="Top" Width="72"/>
        <TextBox x:Name="tbx_email" HorizontalAlignment="Left" Height="24" Margin="302,179,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="146"/>
        <PasswordBox x:Name="tbx_password" HorizontalAlignment="Left" Height="25" Margin="302,217,0,0" VerticalAlignment="Top" Width="146"/>
        <Button x:Name="btn_login" Content="Login" HorizontalAlignment="Left" Height="21" Margin="286,296,0,0" VerticalAlignment="Top" Width="114" Click="btn_login_Click"/>

    </Grid>

</Window>
